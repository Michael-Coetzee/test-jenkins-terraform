pipeline {
    agent { label 'cordfinance' } 
    stages { 
        stage('pull image'){
            steps {
                script {
                    docker.withRegistry('terraformtest.azurecr.io/terraform-az', 'test-terraform') {
                        image = docker.image('terraform-az:latest')
                        image.pull()
                    }
                }
            }
        }
    }
}